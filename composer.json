{
  "name": "reliv/pipe-rat-2",
  "description": "Create REST APIs with just a few lines of Expressive config. This PSR7 compliant PHP library that uses Zend\Expressive Middleware at its core.",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "middleware",
    "rest",
    "module",
    "api",
    "Zend",
    "Expressive"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/reliv/pipe-rat",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/jerv13"
    },
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "https://github.com/rodmcnew"
    }
  ],
  "require": {
    "php": ">=7",
    "zendframework/zend-stratigility": "2.*",
    "zendframework/zend-inputfilter": ">=2",
    "reliv/zf-config-factories": "3.* || 4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "suggest": {
    "rcm/user": "rcm/user for user and ACL",
    "reliv/zf-input-filter-service": "reliv/zf-input-filter-service for input filtering with services"
  },
  "autoload": {
    "psr-4": {
      "Reliv\\PipeRat\\": "src/"
    }
  }
}
