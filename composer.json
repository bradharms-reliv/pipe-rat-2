{
  "name": "reliv/pipe-rat-2",
  "description": "Create REST APIs with just a few lines of Expressive config. This PSR7 compliant PHP library that uses Zend\Expressive Middleware at its core.",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "middleware",
    "rest",
    "module",
    "api",
    "Zend",
    "Expressive"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/reliv/pipe-rat",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/jerv13"
    },
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "https://github.com/rodmcnew"
    }
  ],
  "require": {
    "php": ">=7",
    "zendframework/zend-expressive": "^2.0",
    "reliv/zf-config-factories": "3.* || 4.*",
    "reliv/zf-input-filter-service": "0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "suggest": {
    "rcm/user": "rcm/user for user and ACL"
  },
  "autoload": {
    "psr-4": {
      "Reliv\\PipeRat2\\Acl\\": "acl/src/",
      "Reliv\\PipeRat2\\Core\\": "core/src/",
      "Reliv\\PipeRat2\\DataExtractor\\": "data-extractor/src/",
      "Reliv\\PipeRat2\\DataHydrator\\": "data-hydrator/src/",
      "Reliv\\PipeRat2\\DataValidate\\": "data-validate/src/",
      "Reliv\\PipeRat2\\Options\\": "options/src/",
      "Reliv\\PipeRat2\\Repository\\": "repository/src/",
      "Reliv\\PipeRat2\\RepositoryDoctrine\\": "repository-doctrine/src/",
      "Reliv\\PipeRat2\\RequestAttribute\\": "request-attribute/src/",
      "Reliv\\PipeRat2\\RequestFormat\\": "request-format/src/",
      "Reliv\\PipeRat2\\ResponseFormat\\": "response-format/src/",
      "Reliv\\PipeRat2\\ResponseHeaders\\": "response-headers/src/",
      "Reliv\\PipeRat2\\XampleRepositoryDoctrine\\": "xample-repository-doctrine/src/"
    }
  }
}
